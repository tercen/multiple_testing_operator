{
  "name": "Multiple Testing",
  "description": "Applies a multiple testing correction to your data.",
  "tags": [
    "statistical testing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/multiple_testing_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "bonferroni",
      "values": [
        "bonferroni",
        "BH",
        "holm",
        "hochberg",
        "hommel",
        "BY",
        "qvalue"
      ],
      "description": "Correction method."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor.",
            "ontologyMapping": "variable",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor.",
            "ontologyMapping": "observation",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "P-Value",
                "type": "numeric",
                "description": "P-values to adjust for multiple testing.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}